{
  "name": "my-webpack-project",
  "version": "1.0.0",
  "description": "My webpack project",
  "main": "index.js",
  "scripts": {
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --env mode=development",
    "build:prod": "webpack --env mode=production",
    "watch": "webpack --watch",
    "serve": "webpack serve",
    "start": "webpack serve --env port=4001",
    "lint:ts": " eslint \"**/*.{ts,tsx,js,jsx}\"",
    "lint:ts:fix": " eslint \"**/*.{ts,tsx,js,jsx}\" --fix",
    "lint:scss": " eslint \"**/*.{ts,tsx,js,jsx}\"",
    "lint:scss:fix": " eslint \"**/*.{ts,tsx,js,jsx}\" --fix",
    "storybook": "storybook dev -p 6010 -c ./config/storybook",
    "storybook:build": "storybook build -c ./config/storybook",
    "test:ui": "npx loki test",
    "test:ui:ci": "npx loki --requireReference --reactUri file:./storybook-static",
    "test:ui:ok": "npx loki approve",
    "visual:report:json": "node scripts/generate-visual-json-report.js",
    "visual:report:html": "reg-cli --from .loki/report.json --report .loki/report.html"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.22.10",
